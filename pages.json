---
layout: null
---
{
  "pages": [
    {% for page in site.pages %}
    {% if page.title and page.title != '' %}
    {
      "path": "{{ page.url | remove_first: '/' }}",
      "title": "{{ page.title | escape }}"
    }{% unless forloop.last %},{% endunless %}
    {% endif %}
    {% endfor %}
  ]
}